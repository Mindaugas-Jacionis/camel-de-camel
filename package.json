{
  "name": "camel-de-camel",
  "version": "0.1.0",
  "description": "Recursive camelize or decamelize Objects and/or Arrays of Objects",
  "keywords": [
    "camelize",
    "decamelize",
    "convert",
    "transform",
    "change-case",
    "camelCase",
    "PascalCase",
    "snake_case",
    "kebab-case",
    "UPPER_CASE",
    "dot.case"
  ],
  "homepage": "https://github.com/mindaugas-jacionis/camel-de-camel",
  "bugs": {
    "url": "https://github.com/mindaugas-jacionis/camel-de-camel/issues"
  },
  "repository": "git@github.com:mindaugas-jacionis/camel-de-camel.git",
  "author": "Mindaugas Jaƒçionis",
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "unpkg": "lib/umd/index.js",
  "files": [
    "lib/*",
    "!lib/**/*.js.map"
  ],
  "scripts": {
    "qa": "run-p test lint",
    "test": "jest",
    "clean": "rm -rf lib/",
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "build:cjs/umd": "rollup -c",
    "build:esm": "babel src --out-dir lib/esm --ignore 'src/**/*.test.js' --source-maps"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.41.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "jest": {
    "verbose": true
  }
}
